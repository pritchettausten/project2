

    <div class="parallax-container">
        <div class="parallax">
            <img src="assets/images/explore-utah.jpg">
        </div>
        <h2 class="parallax-text white-text">Explore Utah</h2>
    </div>

    <nav>
        <div class="nav-wrapper">
            <a href="#!" class="brand-logo center">
                <img src="/assets/images/logo-icon.png" alt="logo" class="responsive-img logo-img">
            </a>
            <a href="#!" class="brand-logo center">
                <img src="/assets/images/logo-icon.png" alt="logo" class="responsive-img logo-img">
            </a>
            <ul class="right">
                <li>
                    <a class="hide" id="logout">
                        <i class="material-icons">close</i>
                    </a>
                </li>
                <li id="append">
                    
                    <a class="hide" id="profile">
                        <i class="material-icons">person</i>
                    </a>

                    <a class="modal-trigger" id="loginModal" href="#login-modal">
                        <i class="material-icons">person</i>
                    </a>
                </li>
                <li>
                    <a class="modal-trigger hide" id="create" href="#create-modal">
                        <i class="material-icons">add_circle</i>
                    </a>
                </li>
            </ul>
        </div>
    </nav>

    <div id="create-modal" class="modal white">
        <div class="modal-content">
            <div class="row">
                <h4>Create a New Place</h4>
                <form class="col s12">
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="location_name" type="text" class="validate">
                            <label for="location_name">Location Name</label>
                        </div>
                        <div class="input-field col s6">
                            <input placeholder="Select Activity" id="activity_name" type="radio" class="validate">
                            <select>
                                <option value="">Which Activity?</option>
                                <option value="1">Hiking</option>
                                <option value="2">Biking</option>
                                <option value="3">Climbing</option>
                                <option value="4">Camping</option>
                                <option value="5">Kayaking</option>
                                <option value="6">Skiing</option>
                                <option value="7">Swimming</option>
                                <option value="8">Fishing</option>
                            </select>
                            <label>Select Activity</label>
                        </div>
                        <div class="file-field input-field col s6">
                            <div>
                                <input type="file" id="activityPic">
                            </div>
                            <div class="file-path-wrapper">
                                <i class="material-icons prefix">publish</i>
                                <input placeholder="Add Image" class="file-path validate" type="text">
                            </div>
                        </div>
                        <div class="input-field col s12">
                            <textarea id="write-post" class="materialize-textarea" data-length="250"></textarea>
                            <label for="write-post">Write a Post</label>
                        </div>
                    </div>
                </form>
                <a href="#!" class="modal-action modal-close waves-effect waves-light btn right" id="submit">Submit</a>
            </div>
        </div>
    </div>

    <div id="login-modal" class="modal white">
        <div class="modal-content">
            <div class="row">
                <div class="col s12">
                    <ul class="tabs">
                        <li class="tab col s3">
                            <a class="active" href="#login">Login</a>
                        </li>
                        <li class="tab col s3">
                            <a href="#create-user">New User</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div id="login" class="col s12">
                <div class="row">
                    <h4>Login</h4>
                    <form class="col s12">

                        <div class="row">
                            <div class="input-field col s12">
                                <input id="login-name" type="text" class="validate">
                                <label for="login-name">Username/Email</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s12">
                                <input id="login-password" type="password" class="validate">
                                <label for="login-password">Password</label>
                            </div>
                        </div>
                    </form>
                    <div class="row">
                        <div class="col s6"></div>
                        <div class="col s3">

                            <a href="#!" class="modal-action modal-close waves-effect waves-light btn right" id="login">Login</a>

                        </div>
                    </div>
                </div>
            </div>
            <div id="create-user" class="col s12">
                <h4>Create User</h4>
                <div class="row">
                    <form class="col s12">
                        <div class="row">
                            <div class="input-field col s12">
                                <input id="full-name" type="text" class="validate">
                                <label for="full_name">Full Name</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s12">
                                <input id="email" type="email" class="validate">
                                <label for="email">Email</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s12">
                                <input id="password" type="password" class="validate">
                                <label for="password">Password</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s12">
                                <textarea id="about" class="materialize-textarea" data-length="250"></textarea>
                                <label for="about">About You</label>
                            </div>
                        </div>

                        <div class="file-field input-field col s6">
                            <div>
                                <input type="file" id="picture">
                            </div>
                            <div class="file-path-wrapper">
                                <i class="material-icons prefix">publish</i>
                                <input placeholder="Add Image" class="file-path validate" type="text">
                            </div>
                        </div>                        

                    </form>

                    <a href="#!" class="modal-action modal-close waves-effect waves-light btn right" id="click">Create User</a>

                    {{!-- <a href="/user" class="waves-effect waves-light btn right" id="user">User</a> --}}

                </div>
            </div>
        </div>
    </div>

