        <div class="container">
{{#each post}}
            <div class="row">
                <div class="col s3 m3 l3">
                    {{!-- <div class="star-rating center">****</div> --}}
                    <img src="{{this.picture}}" alt="this is a test" class="responsive-img right location-pictures">
                </div>

                <div class="col s9 m9 l9">
                    <h4>{{this.locationName}}</h4>
                    <p class="flow-text">{{this.body}}</p>
                </div>
            </div>

            <div class="divider"></div>
{{/each}}
