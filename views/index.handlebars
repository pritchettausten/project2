    <div class="row wide-container">
        <!-- <h4 class="blue-text"> Search for an activity </h4>
            <div class="chips chips-autocomplete"></div> -->
        <div class="col s12 m12 l12">

            <h2 class="center-align">Activities Near You</h2>

            <div class="divider"></div>
        </div>
        {{#each review}}
        <div class="row">
                <div class="col s3 m3 l3">
                    <img src="{{this.picture}}" alt="this is a test" class="responsive-img  right location-pictures">
                </div>
                
                <div class="col s9 m9 l9">
                    <div class="row">
                        <div class="col s8 m8 l8">
                            <h4>{{this.location}}</h4>
                        </div>
                        <div class="col s4 m4 l4">
                            <div class="star-rating">{{this.rating}}</div>
                        </div>
                        </div>
                    <p class="location-description flow-text">{{this.body}} </p>
                    <div class="divider"></div>
                </div>
            </div>

        {{/each}}
    </div>

        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input placeholder="Location" id="location_name" type="text" class="validate">
                        <label for="location_name">Location Name</label>
                    </div>
                    <div class="input-field col s6">
                            <input placeholder="Select Activity" id="activity_name" type="radio" class="validate">
                        <select id="activity_select">
                            <option value="" disabled selected>Which Activity?</option>
                            <option value="1">Hiking</option>
                            <option value="2">Biking</option>
                            <option value="3">Climbing</option>
                            <option value="4">Camping</option>
                            <option value="5">Kayaking</option>
                            <option value="6">Skiing</option>
                            <option value="7">Swimming</option>
                            <option value="8">Fishing</option>
                        </select>
                        <label>Select Activity</label>
                    </div>
                    <div class="input-field col s12">
                        <textarea id="write-post" class="materialize-textarea" data-length="250"></textarea>
                        <label for="write-post">Write a Post</label>
                    </div>
                    {{!-- <a class="waves-effect waves-light btn">button</a> --}}
                    <button type="submit" class="btn btn-success submit" id="submit">Submit</button>
                     
                </div>
            </form>
        </div>

